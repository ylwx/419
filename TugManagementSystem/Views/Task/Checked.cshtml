<script type="text/javascript">
    var tableConfig = null;

    $(document).ready(Init);

    function Init() {
        {

            if (tableConfig == null) {
                GetTableConfig();
            }

            $("#jqGrid").jqGrid({
                url: "/Task/GetCheckedData",
                mtype: "GET",
                datatype: "json",
                colModel: tableConfig.columns,
                viewrecords: true,
                //width: 780,
                height: '100%', //height: $(window).height() - 300,
                rowNum: 30,
                autowidth: true,
                editurl: '@Url.Action("", "")',
                pager: "#jqGridPager",
                loadonce: true,
                rowList: ['30', '50', '80', '100'],
                rownumbers: true,
                shrinkToFit: false,     //指定各列宽度
                sortable: true,          //列可移动
                colMenu: false,          //列菜单
                onSelectRow: function (rowid, status, e) {
                    //alert(rowid)
                    //alert(status);
                    var grid = $("#jqGrid");
                    var rowKey = grid.getGridParam("selrow");

                    if (!rowKey) {
                        $('#jqGrid_RepealSubmit').addClass('ui-state-disabled');
                        $('#jqGrid_RepealPass').addClass('ui-state-disabled');
                    }
                    else {
                        var id = $('#jqGrid').jqGrid('getGridParam', 'selrow');
                        var rowData = $('#jqGrid').jqGrid('getRowData', id);
                        var curPhase = rowData.Phase;
                        if (curPhase != -1)
                        {
                            $('#jqGrid_RepealSubmit').removeClass('ui-state-disabled');
                            $('#jqGrid_RepealPass').removeClass('ui-state-disabled');

                        }

                        //var selectedIDs = grid.getGridParam("selarrrow");
                        //if (selectedIDs.length != 1) {
                        //    $('#jqGrid_ilAdd').addClass('ui-state-disabled');
                        //    $('#jqGrid_ilView').addClass('ui-state-disabled');
                        //}
                        //else {
                        //    $('#jqGrid_ilAdd').removeClass('ui-state-disabled');
                        //    $('#jqGrid_ilView').removeClass('ui-state-disabled');
                        //}

                    }
                },
            });

            // 撤销通过
            $("#RepealPass_btn").click(function () {
                var id = $('#jqGrid').jqGrid('getGridParam', 'selrow');
                var rowData = $('#jqGrid').jqGrid('getRowData', id);
                var curPhase = rowData.Phase;
                if (rowData.length) {
                    if (curPhase == -1) {
                        alert("已完成全部审核，无法撤销！");
                    }
                    else {
                        $.ajax(
                                                      {
                                                          type: "post",
                                                          url: "/Task/RepealPass",
                                                          data: { 'data': rowData },
                                                          traditional: true,
                                                          dataType: 'json',
                                                          success: function (result) {
                                                              alert(result.message);
                                                              $('#jqGrid').jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                                                          },
                                                          error: function (result) {
                                                              console.log(result)
                                                              alert(result.message);
                                                          }
                                                      });
                    }

                }
                else { alert("未选择需要操作的数据！"); }
            })

            // 撤销提交
            $("#RepealSubmit_btn").click(function () {
                var rowid = jQuery('#jqGrid').jqGrid('getGridParam', 'selrow');
                var dr = jQuery('#jqGrid').jqGrid('getRowData', rowid);//选中行的数据
                if (rowid != null) {
                    if (confirm('确定要撤销提交吗?')) {
                        var d = new Date();
                        $.ajax(
                           {
                               type: "post",
                               url: "/Task/RepealSubmit",
                               data: { 'data': dr },
                               //traditional: true,
                               dataType: 'json',
                               success: function (result) {
                                   alert(result.message);
                                   $('#jqGrid').jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                               },
                               error: function (result) {
                                   console.log(result)
                                   alert(result.message);
                               }
                           });
                    }

                }
                else { alert("未选择需要操作的数据！"); }
            })

            $('#jqGrid').jqGrid('filterToolbar', { searchResult: true, searchOperators: true });//搜索框

            $("#jqGrid").jqGrid('bindKeys');  //光标可以控制上下移动

            $('#jqGridContainerBox').resize(function () {
                var width = $('#jqGridContainerBox').width();
                $("#jqGrid").setGridWidth(width);
            });

            $('#jqGrid').navGrid('#jqGridPager', { edit: false, add: false, del: false, refresh: false, view: false, search: false })
                .navButtonAdd('#jqGridPager', {
                    caption: "", title: '@Resources.Language.Refresh', buttonicon: "ui-icon-refresh",
                    onClickButton: function () {
                        $('#jqGrid').jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                    }
                })
                .navButtonAdd('#jqGridPager', {
                    caption: "", title: '@Resources.Language.ClearSearchFilter', buttonicon: "ui-icon-circle-minus",
                onClickButton: function () {
                    var grid = $('#jqGrid')[0];
                    grid.clearToolbar();
                    $('#jqGrid').jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                }
            })
            .navSeparatorAdd("#jqGridPager", { sepclass: "ui-separator", sepcontent: '' })
        .navSeparatorAdd("#jqGridPager", { sepclass: "ui-separator", sepcontent: '' })
            .navButtonAdd('#jqGridPager', {
                id: 'jqGrid_ilView',
                caption: "查看", title: "查看账单和回扣单", buttonicon: "ui-icon-search",
                onClickButton: function () {

                    var rowid = jQuery('#jqGrid').jqGrid('getGridParam', 'selrow');

                    var orderId = jQuery('#jqGrid').jqGrid('getCell', rowid, "OrderID"); //OrderID
                    var timeTypeId = jQuery('#jqGrid').jqGrid('getCell', rowid, "TimeTypeID"); //TimeTypeID
                    var timeTypeValue = jQuery('#jqGrid').jqGrid('getCell', rowid, "TimeTypeValue"); //TimeTypeValue
                    var billingTypeId = jQuery('#jqGrid').jqGrid('getCell', rowid, "BillingTypeID"); //BillingTypeID
                    var billingTypeValue = jQuery('#jqGrid').jqGrid('getCell', rowid, "BillingTypeValue"); //BillingTypeValue
                    var billingTypeLabel = jQuery('#jqGrid').jqGrid('getCell', rowid, "BillingTypeLabel"); //BillingTypeLabel

                    if (billingTypeId == 6 || billingTypeValue == "0" || billingTypeLabel == "全包"
                        || billingTypeId == 7 || billingTypeValue == "1" || billingTypeLabel == "半包") {
                        //alert("全包或半包报表预览");
                        //location.href = '/Report/Invoice_qborbb' + '?OrderID=' + orderId + '&TimeTypeValue=' + timeTypeId
                        open('/Report/Invoice_qborbb' + '?OrderID=' + orderId + '&TimeTypeValue=' + timeTypeValue)
                    }

                    else if (billingTypeId == 8 || billingTypeValue == "2" || billingTypeLabel == "计时") {
                        //alert("计时报表预览");
                        open('/Report/Invoice_tk' + '?OrderID=' + orderId + '&TimeTypeValue=' + timeTypeValue)
                    }
                },

            })
        .navButtonAdd('#jqGridPager', {
            id: 'jqGrid_RepealSubmit',
            caption: "撤销提交", title: "撤销提交", buttonicon: "ui-icon-disk",
            onClickButton: function () {
                var rowid = jQuery('#jqGrid').jqGrid('getGridParam', 'selrow');
                var dr = jQuery('#jqGrid').jqGrid('getRowData', rowid);//选中行的数据
                if (rowid != null) {
                    if (confirm('确定要撤销提交吗?')) {
                        var d = new Date();
                        $.ajax(
                           {
                               type: "post",
                               url: "/Task/RepealSubmit",
                               data: { 'data': dr },
                               //traditional: true,
                               dataType: 'json',
                               success: function (result) {
                                   alert(result.message);
                                   $('#jqGrid').jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                               },
                               error: function (result) {
                                   console.log(result)
                                   alert(result.message);
                               }
                           });
                    }
                }
            },

        })
        .navButtonAdd('#jqGridPager', {
            id: 'jqGrid_RepealPass',
            caption: "撤销通过", title: "撤销通过", buttonicon: "ui-icon-disk",
            onClickButton: function () {
                var id = $('#jqGrid').jqGrid('getGridParam', 'selrow');
                var rowData = $('#jqGrid').jqGrid('getRowData', id);
                var curPhase = rowData.Phase;
                if (rowData != null) {
                    if (curPhase == -1) {
                        alert("已完成全部审核，无法撤销！");
                    }
                    else {
                        $.ajax(
                                                      {
                                                          type: "post",
                                                          url: "/Task/RepealPass",
                                                          data: { 'data': rowData },
                                                          traditional: true,
                                                          dataType: 'json',
                                                          success: function (result) {
                                                              alert(result.message);
                                                              $('#jqGrid').jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                                                          },
                                                          error: function (result) {
                                                              console.log(result)
                                                              alert(result.message);
                                                          }
                                                      });
                    }

                }
                else { alert("未选择需要操作的数据！"); }
            },

        });

        $('#jqGrid').inlineNav('#jqGridPager',
            {
                edit: false,
                add: false,
                del: false,
                cancel: false,
                editParams: {
                    keys: true,
                    successfunc: function (val) {
                        if (val.responseText != "") {
                            var ret = $.parseJSON(val.responseText);
                            alert(ret.message);
                            $(this).jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                        }
                    },
                    url: null, //'/api/values',
                    mtype: 'POST'
                },

                addParams: {
                    keys: true,
                    successfunc: function (val) {
                        if (val.responseText != "") {
                            var ret = $.parseJSON(val.responseText);
                            alert(ret.message);
                            $(this).jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                        }
                    },
                    errorfunc: function (rowid, xhr) {
                        alert(xhr.responseText);
                        $(this).jqGrid('setGridParam', { datatype: 'json' }).trigger('reloadGrid');
                    }
                }
            });

        //最后调用，冻结列
    }
    }
